# SET : don't allow duplicate , follows the order ,

s=set()

print(type(s))

s.add(1)# 1 added
print(s)

s = {1,2,3,4,5}
print(s)
print(type(s))

s1= {1,2,3,4,4}
print('removes duplicate value::',s1)#{1,2,3,4}
print(type(s1))

s1.remove(4)
print('removing value 4 ::',s1)#{1,2,3}

s1.pop()
print('pop will removes left most value::',s1)
